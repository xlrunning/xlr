                                    <option>山西</option>
                                    <option>四川</option>
                                    <option>天津</option>
                                    <option>新疆</option>
                                    <option>云南</option>
                                    <option>浙江</option>
                                </select>
                            </div><!-- /.form-group -->
                        </div>
                        <div class="col-md-5">
                            <div class="form-group">
                                <select class="rs-selectize-single">
                                    <option value="3">北京</option>
                                    <option value="5">广州</option>
                                </select>
                            </div><!-- /.form-group -->
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2 title">
                            选择车型
                        </div>
                        <div class="col-md-3">
                            <select class="form-control">
                                <option selected>车型</option>
                                <option value="5">安驰</option>
