                                    <option>江苏</option>
                                    <option>江西</option>
                                    <option>吉林</option>
                                    <option>辽宁</option>
                                    <option>内蒙古</option>
                                    <option>宁夏</option>
                                    <option>青海</option>
                                    <option>陕西</option>
                                    <option>山东</option>
