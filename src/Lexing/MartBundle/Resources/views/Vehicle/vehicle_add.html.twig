{% extends 'mart_admin/base.html.twig' %}
{% block stylesheets %}
    <style>
        .row .title {
            color: #666;
            height: 40px;
            line-height: 40px;
        }
        .margin-top-15{
            margin-top: 15px;
        }
    </style>
    <link rel="stylesheet" href="{{ asset("/themes/roosa/plugins/selectize/css/selectize.min.css") }}">
    <link rel="stylesheet" href="{{ asset("/themes/roosa/plugins/datepicker/css/datepicker.min.css") }}">
{% endblock %}
{% block content %}
